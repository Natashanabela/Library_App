<!-- rounded_rectangle.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="12dp" /> <!-- Atur radius sesuai keinginan untuk mendapatkan bentuk rounded -->
    <solid android:color="#d1aa72" /> <!-- Atur warna latar belakang -->
    <stroke
        android:width="2dp"
    android:color="@android:color/darker_gray" />
</shape>
